    "@react-stately/data": "3.11.0",
    "@uidotdev/usehooks": "2.4.1",
    "@vercel/analytics": "1.1.1",
    "framer-motion": "10.16.5",
    "lodash": "4.17.21",
    "next": "13.5.4",
    "primereact": "10.0.9",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "4.11.0",
    "react-query": "3.39.3",
    "react-responsive-carousel": "3.2.23",
    "sharp": "0.32.6"
  },
  "devDependencies": {
    "@commitlint/cli": "17.7.2",
